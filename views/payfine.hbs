<div class="row">
   <div class="col s12 m12">
     <div class="card white darken-1">
       <div class="card-content black-text">
         <span class="card-title">Fine Details</span>
         <table class="striped">
           <tr>
             <th> Fees Type</th>
             <th>Semester</th>
             <th>Last Date</th>
              <th>D Type</th>
              <th> Graduate</th>
              <th>Fine </th>
              <th>S date</th>
               <th>Super Fine</th>
           </tr>
           {{#each data}}
            <tr class="statictr1">
              <td style="display:none" id="fineid">{{this._id}}</td>
              <td id="eid"> {{this.fees_type}}</td>
              <td id="eid"> {{this.semester}}</td>
              <td id="pos"><input type="text" id="lastd" name="ldate" value="{{this.last_date}}"></td>
              <td id="dep"> {{this.department_type}}</td>
              <td id="con"> {{this.graduate}}</td>
              <td id="con"> <input type="text" id="famount" name="fine" value="{{this.fine}}"></td>
              <td id="con"><input type="text" id="sfdate" name="fine" value=" {{this.superfine_date}}"></td>
              <td id="con"> <input type="text" id="sfamount" name="fine" value=" {{this.superfine}}"></td>
                <td > <input id="fineup" type="submit" value="update"></td>
            </tr>

            {{/each}}
         </table>
       </div>
       <div class="card-action">
         <a href="#">load-more >></a>

       </div>
     </div>
   </div>
 </div>
