


{{#each data}}
<div class="row">
   <div class="col s12 m12">
     <div class="card white darken-1">
       <div class="card-content black-text">
         <span class="card-title">Complaint</span>
         <p>{{this.complaint}}</p>
       </div>
       <div class="card-action">
          <span class="cid" style="display:none">{{this._id}}</span>
         <a href="#" class="del"><i class= "material-icons">delete</i> delete</a>

       </div>
     </div>
   </div>
 </div>
{{/each}}
